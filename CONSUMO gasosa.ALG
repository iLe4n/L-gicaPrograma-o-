Algoritmo "consumo"



Var
          d, g, m : real
          



Inicio

          escreva("Distancia percorrida em KM é ")
          leia(d)
          escreva("Combustivel gasto: ")
          leia(g)
          m <- d/g
          Escreva("Consumo médio foi de:", m:5:3)

       
       
Fimalgoritmo