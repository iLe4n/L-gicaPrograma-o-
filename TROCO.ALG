Algoritmo "troco"



Var

   troco, x, y, preco, quantidade, dr, z : real
          




Inicio

      escreva("Preço unitário do produto é: ")
      leia(x)
      Escreva("Quantidade comprada: ")
      leia(y)
      escreva("Dinheiro recebido: ")
      leia(z)
      Troco <- (z - (x * y))
      Escreva("Seu troco deu ", troco:8:2)
       
       
       
Fimalgoritmo